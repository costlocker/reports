{
    "name": "costlocker/reports",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Costlocker\\Reports\\": "src/Reports",
            "Costlocker\\Reports\\CLI\\": "src/CLI",
            "Costlocker\\Reports\\Custom\\": "src/CustomReports"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/hamcrest/hamcrest-php/hamcrest/Hamcrest.php"
        ]
    },
    "config": {
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/costlocker/PhpSpreadsheet"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony/console": "~3",
        "guzzlehttp/guzzle": "~6",
        "phpoffice/phpspreadsheet": "dev-issue-1056-maxifs-minifs-countifs",
        "swiftmailer/swiftmailer": "~6",
        "google/apiclient": "^2.2",
        "opis/json-schema": "~1",
        "twig/twig": "~1",
        "vlucas/phpdotenv": "~2",
        "monolog/monolog": "^1.24"
    },
    "require-dev": {
        "phpunit/phpunit": "~5",
        "hamcrest/hamcrest-php": "^2.0"
    }
}
